{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [{ "source": "/(.*)", "destination": "/index.html" }],
  "builds": [
    {
      "src": "src/main.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ]
}
